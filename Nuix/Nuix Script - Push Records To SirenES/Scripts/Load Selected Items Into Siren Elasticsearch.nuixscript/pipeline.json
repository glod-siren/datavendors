                "ignore_failure": true
            }
        },
        {
            "gsub": {
                "field": "MetaRevisionNumber",
                "pattern": ",",
                "replacement": "",
                "ignore_failure": true
            }
        },
        {
            "convert": {
                "field": "MetaRevisionNumber",
                "type": "long",
                "ignore_failure": true
            }
        },
